{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": [
      "ES2022"
    ],
    "allowJs": true,
    "checkJs": false,
    "outDir": "_build/tsc-out",
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "resource:///com/github/Aylur/ags/*": [
        "./src/*"
      ],
      "gi://Gtk?version=3.0": [
        "./node_modules/@girs/gtk-3.0"
      ],
      "gi://GObject": [
        "./node_modules/@girs/gobject-2.0"
      ],
      "gi://Gio": [
        "./node_modules/@girs/gio-2.0"
      ],
      "gi://GLib": [
        "./node_modules/@girs/glib-2.0"
      ],
      "gi://GdkPixbuf": [
        "./node_modules/@girs/gdkpixbuf-2.0"
      ],
      "gi://Gdk": [
        "./node_modules/@girs/gdk-2.0"
      ],
      "gi://Gvc": [
        "./node_modules/@girs/gvc-1.0"
      ],
      "gi://NM": [
        "./node_modules/@girs/nm-1.0"
      ],
      "gi://DbusmenuGtk3": [
        "./node_modules/@girs/dbusmenugtk3-0.4"
      ],
      "gi://Soup?version=3.0": [
        "./node_modules/@girs/soup-3.0"
      ]
    },
  },
  "include": [
    "types/*",
    "src"
  ]
}
